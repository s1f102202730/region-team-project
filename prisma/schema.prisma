generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  password  String
  role      String
  createdAt DateTime @default(now()) @map("created_at")
  username  String   @unique @map("username")

  @@map("users")
}
